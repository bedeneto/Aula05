def soma(n1, n2):
    soma1 = print(n1+n2)
    return soma1


def subt(n1, n2):
    subt1 = print(n1 - n2)
    return subt1


def mult(n1,n2):
    mult1 = print(n1 * n2)
    return mult1


def divi(n1,n2):
    divi1 = print(n1 / n2)
    return divi1


n1 = float(input('Informe um número: '))
n2 = float(input('Informe outro número: '))

soma1 = soma(n1,n2)
subt1 = subt(n1,n2)
mult1 = mult(n1,n2)
divi1 = divi(n1,n2)
